@model ICollection<Fisabrantes.Models.Funcionarios>

@{
    ViewBag.Title = "ListaFuncionarios";
}

<h2>Lista de Funcionários</h2>


<div class="form-horizontal">

    @foreach (var idFuncionario in Model)
    {
        <div class="form-group">
            @Html.LabelFor(model => idFuncionario.Nome, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DisplayFor(model => idFuncionario.Nome, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

        <div class="form-group">
            @Html.LabelFor(model => idFuncionario.DataEntClinica, htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-10">
                @Html.DisplayFor(model => idFuncionario.DataEntClinica, new { htmlAttributes = new { @class = "form-control" } })
            </div>
        </div>

        <hr />

    }

</div>